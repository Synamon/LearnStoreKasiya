<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".Factors_Tutorial_Questions">

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/tVRandNum"
        android:text=""
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT1"
        android:layout_marginTop="73dp"
        android:layout_below="@+id/tVRandNum"
        android:layout_toLeftOf="@+id/tVRandNum"
        android:layout_toStartOf="@+id/tVRandNum" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx1"
        android:layout_alignBottom="@+id/eT1"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT1"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT2"
        android:layout_alignTop="@+id/eT1"
        android:layout_toRightOf="@+id/tVRandNum"
        android:layout_toEndOf="@+id/tVRandNum" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT3"
        android:layout_below="@+id/eT1"
        android:layout_alignLeft="@+id/eT1"
        android:layout_alignStart="@+id/eT1"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx2"
        android:layout_alignBottom="@+id/eT3"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT3"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT4"
        android:layout_alignBottom="@+id/tVx2"
        android:layout_alignLeft="@+id/eT2"
        android:layout_alignStart="@+id/eT2" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT5"
        android:layout_below="@+id/eT3"
        android:layout_alignLeft="@+id/eT1"
        android:layout_alignStart="@+id/eT1"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx3"
        android:layout_alignBottom="@+id/eT5"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT5"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT6"
        android:layout_alignBottom="@+id/tVx3"
        android:layout_alignLeft="@+id/eT2"
