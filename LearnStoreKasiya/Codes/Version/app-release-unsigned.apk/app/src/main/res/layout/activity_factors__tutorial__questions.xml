    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT7"
        android:layout_below="@+id/eT5"
        android:layout_alignLeft="@+id/eT1"
        android:layout_alignStart="@+id/eT1"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx4"
        android:layout_alignBottom="@+id/eT7"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT7"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT8"
        android:layout_alignBottom="@+id/tVx4"
        android:layout_alignLeft="@+id/eT2"
        android:layout_alignStart="@+id/eT2" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT9"
        android:layout_below="@+id/eT7"
        android:layout_alignLeft="@+id/eT1"
        android:layout_alignStart="@+id/eT1"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx5"
        android:layout_alignBottom="@+id/eT9"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT9"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT10"
        android:layout_alignBottom="@+id/tVx5"
        android:layout_alignLeft="@+id/eT2"
        android:layout_alignStart="@+id/eT2" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT11"
        android:layout_below="@+id/eT9"
        android:layout_alignLeft="@+id/eT1"
        android:layout_alignStart="@+id/eT1"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="50dp"
        android:id="@+id/tVx6"
        android:layout_alignBottom="@+id/eT11"
        android:layout_centerHorizontal="true"
        android:layout_alignBaseline="@id/eT11"/>

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/eT12"
        android:layout_alignBottom="@+id/tVx6"
        android:layout_alignLeft="@+id/eT2"
        android:layout_alignStart="@+id/eT2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x"
        android:visibility="visible"
        android:id="@+id/checktV1"
        android:layout_alignBottom="@+id/tVx1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x"
        android:visibility="invisible"
        android:id="@+id/checktV2"
        android:layout_alignBottom="@+id/tVx2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="x"
        android:id="@+id/checktV3"
        android:visibility="invisible"
        android:layout_alignBottom="@+id/tVx3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
